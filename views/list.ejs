<%- include("header")-%>
    <div class="box" id="heading">
        <H1><%=listTitle%></H1> 
    </div>
    <div class="box">
        <% for(let i=0;i<newListItems.length;i++){ %>
            <div class="item">
                <input type="checkbox" name="" id="">
                <p><%= newListItems[i]%></p>
            </div>
            
        <% } %> 
        <form action="/" method="post">
            <input type="text" name="newItem" id="" placeholder="Add item to list" autofocus autocomplete="off" required>
            <button type="submit" name="list" value= <%=listTitle%> >+</button>
        </form>
    </div> 
<%- include("footer") -%>